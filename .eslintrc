{
	"parser": "babel-eslint",
	"plugins": [
		"react"
  ],
	"extends": "airbnb",
	"globals": {
		"document": true,
		"window": true,
		"localStorage": true
	},
  "rules": {
    "no-console": 0,
		"indent": ["error", "tab"],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
    "no-tabs": 0
  }
}